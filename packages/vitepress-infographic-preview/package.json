{
  "name": "vitepress-infographic-preview",
  "version": "1.0.0",
  "type": "module",
  "description": "A VitePress plugin for previewing AntV Infographic charts",
  "author": "flingyp",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "components"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./component": {
      "types": "./dist/component.d.ts",
      "import": "./dist/component.js",
      "require": "./dist/component.cjs"
    },
    "./dist/index.css": "./dist/vitepress-infographic-preview.css"
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build"
  },
  "dependencies": {
    "@antv/infographic": "^0.2.10",
    "@flypeng/tool": "^5.7.2",
    "@iconify/vue": "^5.0.0",
    "@vueuse/components": "^13.5.0",
    "@vueuse/core": "^13.5.0",
    "@zumer/snapdom": "^1.9.5",
    "vue-sonner": "^2.0.2"
  },
  "peerDependencies": {
    "vitepress": "^1.0.0",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.16.4",
    "@vitejs/plugin-vue": "^6.0.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.5"
  },
  "keywords": [
    "vitepress",
    "plugin",
    "antv",
    "infographic",
    "chart",
    "diagram",
    "markdown"
  ]
}
